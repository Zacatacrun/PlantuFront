form(action="singIn", method="post")
  label(for="nombre") Nombre:
  input(type="text", id="nombre", name="nombre",  placeholder='ingresa usuario', required)

  label(for="contrasena") Contraseña:
  input(type="password", id="contrasena", name="contrasena",  placeholder='ingresa contraseña', required)

  label(for="correo") Correo electrónico:
  input(type="email", id="correo", name="correo",  placeholder='ingresa correo', required)

  label(for="rol") Rol:
  select(id="rol", name="rol", required)
    option(value="administrador") Administrador
    option(value="usuario") Usuario

  button(type="submit") Registrarse